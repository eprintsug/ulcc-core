<?xml version="1.0" ?>

<!-- 
	Brief citation for an eprint. Just gives the title or the type and ID number.
-->

<cite:citation xmlns="http://www.w3.org/1999/xhtml" xmlns:cite="http://eprints.org/ep3/citation" xmlns:epc="http://eprints.org/ep3/control">

<cite:linkhere><epc:choose>
  <epc:when test="content"><epc:print expr="content"/></epc:when> 
  <epc:when test="formatdesc"><epc:print expr="formatdesc"/></epc:when> 
  <epc:otherwise>Document #<epc:print expr="docid"/></epc:otherwise>
</epc:choose></cite:linkhere>

</cite:citation>
